{
  "name": "e2e-staking-dashboard",
  "scripts": {
    "test--": "nightwatch --report ./html-reporter.js",
    "test": "env NODE_ENV=test nightwatch",
    "server": "env NODE_ENV=test node ./test-server.js",
    "show-report": "nightwatch-html-reporter -d ./reports -t outlook -l 0",
    "report": "nightwatch-html-reporter -d ./reports -t outlook -l 0 -b false -o ./index.html"
  },
  "devDependencies": {
    "chromedriver": "^2.36.0",
    "handlebars": "^4.7.3",
    "nightwatch": "^0.9.20"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "dotenv-expand": "^5.1.0",
    "express": "^4.17.1",
    "nightwatch-html-reporter": "^2.0.6",
    "selenium-webdriver": "^4.0.0-alpha.7"
  }
}
